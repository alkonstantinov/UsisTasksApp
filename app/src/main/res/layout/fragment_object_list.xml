<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ipltd_bg.usistasksapp.fragment.ObjectListFragment">

    <!-- TODO: Update blank fragment layout -->
    <ListView
        android:id="@+id/lvTasks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="90dp"></ListView>


    <com.rey.material.widget.FloatingActionButton
        android:id="@+id/confirm"
        android:layout_gravity="right|top"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        app:fab_backgroundColor="#dddddd"
        app:fab_iconSrc="@drawable/check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.rey.material.widget.FloatingActionButton>


    <com.rey.material.widget.FloatingActionButton
        android:id="@+id/goBack"
        android:layout_gravity="left|top"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        app:fab_backgroundColor="#dddddd"
        app:fab_iconSrc="@drawable/go_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.rey.material.widget.FloatingActionButton>

    <com.rey.material.widget.FloatingActionButton
        android:id="@+id/toggleShowAll"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginBottom="10dp"
        app:fab_backgroundColor="#dddddd"
        app:fab_iconSrc="@drawable/list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.rey.material.widget.FloatingActionButton>

</FrameLayout>
